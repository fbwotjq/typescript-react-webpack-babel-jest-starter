{
  "name": "typescript-react-webpack-babel-quickstart",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prepublishOnly": "webpack --config webpack.prod.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --config webpack.dev.js --progress --colors --watch --hot --inline"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.1.3",
    "@types/react-test-renderer": "^16.0.1",
    "awesome-typescript-loader": "^3.5.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "circular-dependency-plugin": "^4.4.0",
    "clean-webpack-plugin": "^0.1.18",
    "compression-webpack-plugin": "^1.1.8",
    "core-js": "^2.5.3",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.10.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "tslint --fix --force",
      "git add"
    ]
  },
  "jest": {
    "roots": ["tests"],
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.ts",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "src",
      "tests",
      "node_modules"
    ]
  }
}
